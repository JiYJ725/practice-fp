.sideMenu
  position fixed
  top 50px
  left 0
  rigth 0
  bottom 0
  width 200px
  height 100%
  color white
  background-color #000000
  z-index 9999
  visibility hidden
  transform translateX(-100%)
  transition transform 0.4s

  @media screen and (max-width: 768px)
    visibility visible

.sideMenu.open
  transform translateX(0px)




